{
  "name": "tfm4",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@3.2.4",
  "scripts": {
    "dev": "yarn workspace @tfm4/front next dev",
    "tsc:watch": "tsc --build tsconfig.references.json --watch",
